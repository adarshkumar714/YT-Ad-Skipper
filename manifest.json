{
    "name" : "yt-ad-skipper",
    "version" : "1.0.0",
    "description" : "A custome chrome extension which bypasses all youtube ads",
    "manifest_version" : 3,
    "author" : "Adarsh Kumar",
    "icons" : {
        "24" : "icons/offstate2.png"
    },
    "content_scripts" : [
        {
            "matches" : [
                "https://www.youtube.com/*"
            ],
            "js" : [
                "content_scripts/content.js"
            ]
        }
    ]
}